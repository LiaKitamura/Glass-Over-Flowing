<!DOCTYPE html>
<html>
<head>
  <title>Glass Over Flowing</title>
  <%= stylesheet_link_tag 'application', media: 'all' %>
  <%= csrf_meta_tags %>
  <%= favicon_link_tag 'favicon.ico' %>
</head>
<body>
  <div id="header">
    <header>
      <div class="navbar">
        <nav class='navbar'>
          <ul class='nav navbar-nav' id="item1">
            <%= render 'layouts/navbar' %>
          </ul>
            <%= render 'layouts/pgsearch' %>
        </nav>
      </div>
    </header>
  </div>
  <%= show_notice_message %>
  <div class="container">
    <section>
      <%= yield %>
    </section>
    <%= render 'layouts/footer' %>
  </div>
  <%= javascript_include_tag 'application' %>
</body>
</html>
